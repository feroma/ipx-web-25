{
  "routes": [
    {
      "route": "/200",
      "rewrite": "/200.html"
    },
    {
      "route": "/404",
      "rewrite": "/404.html"
    },
    {
      "route": "/index.html",
      "redirect": "/"
    },
    {
      "route": "/",
      "rewrite": "/api/server"
    }
  ],
  "platform": {
    "apiRuntime": "node:18"
  },
  "navigationFallback": {
    "rewrite": "/api/server"
  }
}